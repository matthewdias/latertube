{
	"manifest_version": 2,
	
	"name": "latertube",
	"description": "Quick actions for YouTube embeds",
	"version": "1.0",

	"permissions": ["identity", "contextMenus"],

	"key": "",
	"oauth2": {
		"client_id": "315945223165-7ubhdgo9lvokpdqt37g1vf0d8ie07uto.apps.googleusercontent.com",
		"scopes": ["https://www.googleapis.com/auth/youtube"]
	},
	"content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",

	"background": {
		"scripts": ["jquery.js", "gapi-client.js", "auth.js", "background.js"]
	},

	"icons": {
          "16": "icon16.png",
          "48": "icon48.png",
          "128": "icon128.png"
    },

	"commands": {
		"watchlater": {
			"suggested_key": {
				"default": "Ctrl+Shift+L"
			},
			"description": "Add video to Watch Later"
		}
	}
}